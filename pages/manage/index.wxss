/* pages/manage/index.wxss */
.line{
  border-bottom: 0.5px solid #e5e5e5;
}
.section{
  height: 88px;
  width: 100%;
  border-bottom: 0.5px solid #e5e5e5;
  background:#ffffff;
  /* background: gray; */
}
.section .manage-image{
  height: 88px;
  /* border: 1px solid black; */
  /* background: green;  */
  float: left;
  padding-left: 15px;
  padding-right:15px;
}
.section .manageinfo{
  /* background: red;  */
  /* border: 1px solid black; */
  padding-left: 0px;
  height: 88px;
  float: left;
  margin: 0px;
  width:50%; 
}
.section .manage-image image{
  height: 120rpx;
  width: 120rpx;
  margin-top:14px;
  border-radius:5px; 
}
.section .manageinfo .manage-title{
  height: 39px;
  padding-bottom: 5px;
  position: relative;
  width:150px;
  /* background: yellow;   */
  /* padding-top: 10px; */
}
.section .manageinfo .manage-desc{
  height: 44px;
  margin-top:6px;
  /* padding-top: 6px; */
}
.section .manageinfo .manage-title text{
  margin-top: 10px;
  /* line-height: 50px; */
  font-size: 18px;
  color: #353535;
  position:absolute;  
  bottom:0px;  
}
.section .manageinfo .manage-desc text{
  /* margin-top: 10px; */
  font-size:13px;
  color:#888888;
  white-space:nowrap;
  display:block;
  overflow:hidden;
  text-overflow:ellipsis;
}
.section .point{
  height: 88px;
  float: right;
  /* background: yellow; */
}
.section .point image{
  height: 16px;
  width: 12px;
  padding-top: 36px;
  margin-right: 15px;
  padding-right: 0;
}
.section_web{
  height:110px;
  background:#ffffff;
  margin-top:10px;
}
.section_web .webimage{
  display: block;
  height: 110px;
  float: left;
  /* background: blue; */
}
.section_web .webimage image{
  height: 80px;
  width: 70px;
  margin: 15px 12px 15px 15px;  
}
.section_web .webinfo{
  height: 110px;
  display: block;
  float: left;
  /* background: lightblue; */
}
.section_web .webinfo view{
  display: block;
}
.section_web .webinfo .webname image{
  width:36px;
  height:18px;
  margin: 15px 9px 0 0;
}
.webname text{
  font-size: 18px;
  color: #353535;
}
.webip{
  font-size: 13px;
  color: #888888;
  margin-top:4.5rpx;
}
.weburl{
  font-size: 13px;
  color: #888888;
  margin-top:4rpx;
}
.webspace{
  font-size: 13px;
  color: #888888;
  margin-top:4rpx;
}
